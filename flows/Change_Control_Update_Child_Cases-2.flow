    <decisions>
        <name>Check_Change</name>
        <label>Check Change</label>
        <locationX>483</locationX>
        <locationY>224</locationY>
        <defaultConnectorLabel>Default</defaultConnectorLabel>
        <rules>
            <name>Owner_Change</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>vChangeOwner</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loop_Child_Cases</targetReference>
            </connector>
            <label>Owner Change</label>
        </rules>
        <rules>
            <name>Status_Change</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>vChangeStatus</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>Status Change</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_if_Child_Cases_Found</name>
        <label>Check if Child Cases Found</label>
        <locationX>481</locationX>
        <locationY>116</locationY>
        <defaultConnector>
            <targetReference>Check_Change</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Found</defaultConnectorLabel>
        <rules>
            <name>Not_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>scChildCases</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>Not Found</label>
        </rules>
    </decisions>
    <description>Change Control - WHenever Parent Case owner is updated, this flow updates all children &quot;KM Parent Case Owner&quot; field to Parent case&apos;s owner</description>
    <label>Change Control - Update Child Cases</label>
    <loops>
        <name>Loop_Child_Cases</name>
        <label>Loop Child Cases</label>
        <locationX>299</locationX>
        <locationY>360</locationY>
        <assignNextValueToReference>sChildCases</assignNextValueToReference>
        <collectionReference>scChildCases</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Update_Child_Owner_Id</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Update_Child_Cases</targetReference>
        </noMoreValuesConnector>
    </loops>
    <processType>AutoLaunchedFlow</processType>
    <recordCreates>
