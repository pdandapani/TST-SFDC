    <fields>
        <fullName>Special_Handling__c</fullName>
        <description>SRC: Salesforce 
INT: 
Trans: Y 
BU: RL 
FD: Images &amp; text to guide the associate for special circumstances: 
(1) Verify Host Data: when FA last modified &lt; 10 PM Pacific prior day on Tu-Sa or Fri on Su-Mo accounting for daylight savings to calculate when 10 PM Pacific occurs in GMT or when last modified by is not ETL User 
(2) Fraud: when collection list is one of the fraud list numbers 
(3) Active BK: when the bankruptcy flag is one of the active BK flags 
(4) Discharged BK: when the bankruptcy flag is one of the discharged BK flags 
(5) Not Owner: when parent case isn&apos;t owned by the current user 
(6) Account Termed: financial account status is closed</description>
        <externalId>false</externalId>
        <formula>IF( 
Financial_Account__r.LastModifiedDate &lt; 
DATETIMEVALUE( 
TEXT(TODAY() - IF(MOD(TODAY() - DATE(1900, 1, 7), 7) &gt; 1, 0, IF(MOD(TODAY() - DATE(1900, 1, 7), 7) = 1, 2, 1))) 
&amp; 
IF (TODAY() &gt;= 
DATE(YEAR(TODAY()),3,1) 
+ 
( 
14- 
CASE(MOD(DATE(YEAR(TODAY()),3,1) - DATE(1900,1,7),7), 
0,7,MOD(DATE(YEAR(TODAY()),3,1) - DATE(1900,1,7),7)) 
) 
&amp;&amp; 
TODAY() &lt; 
DATE(YEAR(TODAY()),11,1) 
+ 
( 
7- 
CASE(MOD(DATE(YEAR(TODAY()),11,1) - DATE(1900,1,7),7), 
0,7,MOD(DATE(YEAR(TODAY()),11,1) - DATE(1900,1,7),7)) 
), 
&quot; 05:00:00&quot;, 
&quot; 06:00:00&quot; 
) 
) || 
Financial_Account__r.LastModifiedBy.FirstName &amp; Financial_Account__r.LastModifiedBy.LastName &lt;&gt; &quot;ETLUser&quot;, 
IMAGE(&quot;/servlet/servlet.ImageServer?id=01570000002E3dT&amp;oid=00D180000001P5o&quot;, &quot;Verify Host Data&quot;) &amp; &quot; &quot;, &quot;&quot;) 

&amp; 

IF(Financial_Account__r.Collection_List_Number_1__c = &quot;1074&quot; || Financial_Account__r.Collection_List_Number_1__c = &quot;1075&quot; || Financial_Account__r.Collection_List_Number_1__c = &quot;1076&quot; || Financial_Account__r.Collection_List_Number_1__c = &quot;1077&quot;, IMAGE(&quot;/servlet/servlet.ImageServer?id=01570000002E3dQ&amp;oid=00D180000001P5o&quot;, &quot;Fraud&quot;) &amp; &quot; &quot;, &quot;&quot;) 

&amp; 

IF(Financial_Account__r.Bankruptcy_Flag__c = &quot;A&quot; || Financial_Account__r.Bankruptcy_Flag__c = &quot;B&quot; || Financial_Account__r.Bankruptcy_Flag__c = &quot;C&quot; || Financial_Account__r.Bankruptcy_Flag__c = &quot;D&quot; || Financial_Account__r.Bankruptcy_Flag__c = &quot;T&quot;, IF(Voluntary_Surrender__c, IMAGE(&quot;/servlet/servlet.ImageServer?id=01570000002E3dN&amp;oid=00D180000001P5o&quot;, &quot;Active Bankruptcy&quot;), IMAGE(&quot;/servlet/servlet.ImageServer?id=01570000002E3dO&amp;oid=00D180000001P5o&quot;, &quot;Active Bankruptcy-Involuntary Surrender&quot;)) &amp; &quot; &quot;, &quot;&quot;) 

&amp; 

IF(Financial_Account__r.Bankruptcy_Flag__c = &quot;E&quot; || Financial_Account__r.Bankruptcy_Flag__c = &quot;F&quot; || Financial_Account__r.Bankruptcy_Flag__c = &quot;H&quot; || Financial_Account__r.Bankruptcy_Flag__c = &quot;G&quot;, IMAGE(&quot;/servlet/servlet.ImageServer?id=01570000002E3dP&amp;oid=00D180000001P5o&quot;, &quot;Discharged Bankruptcy&quot;) &amp; &quot; &quot;, &quot;&quot;) 
&amp; 

IF($User.Id != Case__r.OwnerId, IMAGE( &quot;/servlet/servlet.ImageServer?id=01570000002E3dR&amp;oid=00D180000001P5o&quot;, &quot;You are not the owner of the case&quot;) &amp; &quot; &quot;, &quot;&quot;)

&amp; 

IF(TEXT(Financial_Account__r.Finance_Account_Status__c) = &quot;Close&quot;, IMAGE(&quot;/servlet/servlet.ImageServer?id=01570000002E3dS&amp;oid=00D180000001P5o&quot;, &quot;Financial account is closed&quot;) &amp; &quot; &quot;, &quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Special Handling</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
