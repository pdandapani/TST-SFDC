    <fields>
        <fullName>Next_Payment_Date__c</fullName>
        <description>SRC: Salesforce
INT: 
Trans: Y
BU: RL
FD: Formula field to calculate next scheduled payment date from Host-fed Financial_Account__c.Payment_Date_Next__c for comparison in validation. Takes into consideration end of year, February short month, February leap year, and payments scheduled on the last day of a month when the next scheduled month only has 30 days, but Financial_Account__c.Payment_Date_Next__c was scheduled on the 31st.</description>
        <externalId>false</externalId>
        <formula>IF(DAY(TODAY()) &gt; DAY(Financial_Account__r.Payment_Date_Next__c),
  DATE(IF(MONTH(TODAY()) = 12,
         YEAR(TODAY()) + 1,
         YEAR(TODAY())
       ),
    IF(MONTH(TODAY()) = 12,
      1,
      MONTH(TODAY()) + 1
    ),
    IF(MONTH(TODAY()) = 1 &amp;&amp; MOD(YEAR(TODAY()), 4) = 0 &amp;&amp; DAY(Financial_Account__r.Payment_Date_Next__c) &gt; 29,
      29,
      IF(MONTH(TODAY()) = 1 &amp;&amp; MOD(YEAR(TODAY()), 4) != 0 &amp;&amp; DAY(Financial_Account__r.Payment_Date_Next__c) &gt; 28,
        28,
        IF(MOD(MONTH(TODAY()) + 1, 2) = 0 &amp;&amp; MONTH(TODAY()) + 1 &lt; 7 &amp;&amp; DAY(Financial_Account__r.Payment_Date_Next__c) = 31,
          30,
          IF(MOD(MONTH(TODAY()) + 1, 2) = 1 &amp;&amp; MONTH(TODAY()) + 1 &lt; 13 &amp;&amp; DAY(Financial_Account__r.Payment_Date_Next__c) = 31,
            30,
            DAY(Financial_Account__r.Payment_Date_Next__c)
          )
        )
      )
    )
  ),

  DATE(YEAR(TODAY()), MONTH(TODAY()),
    IF(MONTH(TODAY()) = 2 &amp;&amp; MOD(YEAR(TODAY()), 4) = 0 &amp;&amp; DAY(Financial_Account__r.Payment_Date_Next__c) &gt; 29,
      29,
      IF(MONTH(TODAY()) = 2 &amp;&amp; MOD(YEAR(TODAY()), 4) != 0 &amp;&amp; DAY(Financial_Account__r.Payment_Date_Next__c) &gt; 28,
        28,
        IF(MOD(MONTH(TODAY()), 2) = 0 &amp;&amp; MONTH(TODAY()) &lt; 7 &amp;&amp; DAY(Financial_Account__r.Payment_Date_Next__c) = 31,
          30,
          IF(MOD(MONTH(TODAY()), 2) = 1 &amp;&amp; DAY(Financial_Account__r.Payment_Date_Next__c) = 31,
            30,
            DAY(Financial_Account__r.Payment_Date_Next__c)
          )
        )
      )
    )
  )
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
