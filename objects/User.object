    <validationRules>
        <fullName>Reqd_LANID_and_RACFID_for_Collections</fullName>
        <active>true</active>
        <description>Users in Collections Profile must have Lan Id or Coll Racf Id field values</description>
        <errorConditionFormula>AND(
  BEGINS( Profile.Name , &quot;Collections&quot;),
  OR(
    ISBLANK(Coll_Racf_Id__c),
    ISBLANK(Lan_Id__c)
  )
)</errorConditionFormula>
        <errorMessage>Users in Collections Profile must have Lan Id or Coll Racf Id field values</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_LANID_and_RACFID_for_Collections</fullName>
        <active>true</active>
        <description>Users in Collections roles must have Lan Id and Coll Racf Id field values</description>
        <errorConditionFormula>AND(
  BEGINS(UserRole.Name, &quot;Collections&quot;),
  OR(
    ISBLANK(Coll_Racf_Id__c),
    ISBLANK(Lan_Id__c)
  )
)</errorConditionFormula>
        <errorMessage>Users in Collections roles must have Lan Id and Coll Racf Id field values</errorMessage>
    </validationRules>
