  NOT(Reentrant_Trigger_Executed__c),
  ISCHANGED(HomePhone_BillTo__c),
  NOT(REGEX(HomePhone_BillTo__c, &quot;\\([0-9]{3}\\)\\s[0-9]{3}\\-[0-9]{4}&quot;))
)</errorConditionFormula>
