    <validationRules>
        <fullName>Prevent_Account_Change</fullName>
        <active>true</active>
        <description>The related account cannot be locked down via page layout or FLS since it is required in a Master-Detail, so validation rule is needed to prevent end users from changing the field value. Only integration or a system administrator may change the name.</description>
        <errorConditionFormula>AND(
  ISCHANGED(Account__c),
  NOT($Profile.Name=&quot;System Administrator&quot;||$Profile.Name=&quot;API Only&quot;||$Profile.Name=&quot;System Administrator Clone&quot;),
  NOT(Reentrant_Trigger_Executed__c)
)</errorConditionFormula>
        <errorMessage>The associated customer (Account) cannot be changed. Cancel your changes and re-edit without modifying the value of the Account field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Blank_Addresses</fullName>
        <active>true</active>
        <description>Prevent users from blanking out required address components</description>
        <errorConditionFormula>AND(
  NOT($Profile.Name=&quot;System Administrator&quot;||$Profile.Name=&quot;API Only&quot;||$Profile.Name=&quot;System Administrator Clone&quot;),
  NOT(Reentrant_Trigger_Executed__c),
  OR(
    AND(
      OR(
        NOT(ISBLANK(PRIORVALUE(Mailing_Street_BillTo__c))),
        NOT(ISBLANK(PRIORVALUE(Mailing_City_BillTo__c))),
        NOT(ISBLANK(PRIORVALUE(Mailing_State_BillTo__c))),
        NOT(ISBLANK(PRIORVALUE(Mailing_PostalCode_BillTo__c)))
      ),
      OR(
        ISBLANK(Mailing_Street_BillTo__c),
        ISBLANK(Mailing_City_BillTo__c),
        ISBLANK(TEXT(Mailing_State_BillTo__c)),
        ISBLANK(Mailing_PostalCode_BillTo__c)
      )
    ),
    OR(
      ISBLANK(Mailing_Street_Garage__c),
      ISBLANK(Mailing_City_Garage__c),
      ISBLANK(TEXT(Mailing_State_Garage__c)),
      ISBLANK(Mailing_Postal_Code_Garage__c)
    )
  )
)</errorConditionFormula>
        <errorMessage>You may not enter blanks in Street, City, State, or Zip Code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_FAC_Name_Change</fullName>
