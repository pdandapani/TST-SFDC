    <validationRules>
        <fullName>Bill_To_Addr_Spec_Inst_Format</fullName>
        <active>true</active>
        <description>Cannot exceed 25 char for Lease and 36 char for Retail.</description>
        <errorConditionFormula>AND(
  NOT($Profile.Name=&quot;System Administrator&quot;||$Profile.Name=&quot;API Only&quot;||$Profile.Name=&quot;System Administrator Clone&quot;),
  NOT(Reentrant_Trigger_Executed__c),
  ISCHANGED(Address_Special_BillTo__c),
  OR(
    LEN(Address_Special_BillTo__c) &gt; 25 &amp;&amp;  Financial_Account__r.RecordType.Name = &quot;Lease&quot;,
    LEN(Address_Special_BillTo__c) &gt; 36 &amp;&amp;  Financial_Account__r.RecordType.Name = &quot;Retail&quot;
)
)</errorConditionFormula>
        <errorDisplayField>Address_Special_BillTo__c</errorDisplayField>
        <errorMessage>Care of/special instructions cannot exceed 25 characters in length for leases or 36 characters for loans</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bill_To_Home_Phone_DNC_Invalid</fullName>
        <active>true</active>
        <description>Both DNC and Invalid flags cannot be set, it should be just 1 of these 2.</description>
        <errorConditionFormula>AND(
  NOT($Profile.Name=&quot;System Administrator&quot;||$Profile.Name=&quot;API Only&quot;||$Profile.Name=&quot;System Administrator Clone&quot;),
  NOT(Reentrant_Trigger_Executed__c),
  OR(
    ISCHANGED(Flag_Home_Phone_DNC_BillTo__c),
    ISCHANGED(Flag_Home_Phone_Invalid_BillTo__c)
  ),
  Flag_Home_Phone_DNC_BillTo__c &amp;&amp; Flag_Home_Phone_Invalid_BillTo__c
)</errorConditionFormula>
        <errorDisplayField>Flag_Home_Phone_Invalid_BillTo__c</errorDisplayField>
        <errorMessage>DNC and Invalid flags cannot be set at the same time - deselect one of them</errorMessage>
    </validationRules>
    <validationRules>
